<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://ww.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context.xsd">
	
	
	
		
	<bean class = "com.springcore.collections.Employe" name="employe">
		<property name="name" value="Vikash Basant" />
		
		<property name="phones">
			<list>
				<value>8789000000</value>
				<value>8788000000</value>
				<value>8787000000</value>
				<value>8786000000</value>
				<null/>
			</list>
		</property>
		
		<property name="addresses">
			<set>
				<value>Bhagwatpur</value>
				<value>Sarairanjan</value>
				<value>Samastipur</value>
			</set>
		</property>
		
		<property name="courses">
			<map>
				<entry key = "c" value="3month"/>
				<entry key = "c++" value="5month"/>
				<entry key = "java" value="10month"/>
				<entry key = "c#" value="3month" />
			</map>
		</property>
		
		<property name="props">
			<props>
				<prop key="name">Vikash basant</prop>
				<prop key="employee">Decimal Technologies</prop>
			</props>
		</property>
	</bean>
	
	
</beans>